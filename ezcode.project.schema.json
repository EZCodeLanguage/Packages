{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Project",
    "type": "object",
    "properties": {
      "Name": {
        "type": "string"
      },
      "Files": {
        "type": "array",
        "items": {
          "type": "string"
        }
      },
      "GlobalPackages": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "nullable": true
      }
    },
    "required": ["Name", "Files"]
  }